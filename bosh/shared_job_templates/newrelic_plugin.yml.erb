newrelic:
  #
  # Update with your New Relic account license key:
  #
  license_key: <%= properties.cc.newrelic.license_key ? "'#{properties.cc.newrelic.license_key}'" : "~" %>
  #
  # Set to '1' for verbose output, remove for normal output.
  # All output goes to stdout/stderr.
  #
  verbose: 1

#
# Agent Configuration, describe your nginx instances here
#
agents:
  nginx_status_agent:
    -
      instance_name: 'CC Nginx (<%= properties.cc.newrelic.environment_name %>)'
      status_url: http://localhost:<%= p("cc.external_port") %>/nginx_status
